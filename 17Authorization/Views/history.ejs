<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>URL History</title>
  </head>
  <body>
    <ul>
      <% urls.forEach(url => { %>
      <li><%= url.shortURL %> -> <%= url.originalURL %></li>
      <% }); %>
    </ul>
  </body>
</html>
